<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>

<GridPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.se_project.AdminViewController"
            prefHeight="400.0" prefWidth="600.0">
    <padding><Insets top="25" right="25" bottom="10" left="25"/></padding>

    <Button fx:id="createUserButton"
            GridPane.columnIndex="0"
            GridPane.rowIndex="1"
            text="Create User Profile"
            onAction="#onCreateUserProfileClick"
    />
    <Button fx:id="updateUserButton"
            GridPane.columnIndex="1"
            GridPane.rowIndex="1"
            text="Update User Profile"
            onAction="#onUpdateUserProfileClick"
    />
    <Button fx:id="deleteUserButton"
            GridPane.columnIndex="0"
            GridPane.rowIndex="2"
            text="Delete User Profile"
            onAction="#onDeleteUserButtonClick"
    />

    <Button fx:id="logoutButton"
            GridPane.columnIndex="10"
            GridPane.rowIndex="10"
            text="Logout"
            onAction="#onlogoutButtonClick"
    />

    <Button fx:id="searchUserButton"
            GridPane.columnIndex="1"
            GridPane.rowIndex="2"
            text="Search for a User Profile"
            onAction="#onsearchUserButtonClick"
    />

</GridPane>
